{
	"compilerOptions": {
		"target": "ES2019",
		"module": "ESNext",
		"lib": ["ES2019", "DOM", "DOM.Iterable"],
		"jsx": "react-jsx",
		"declaration": false,
		"strict": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	},
	"include": ["src/**/*"]
}